программа ДляКаждого(арг: массив строк);
переменная
    к: строка;
    ц: целое;
    сл: словарь словарей целых ключ строка ключ целое;
начало
    сл[0]["первый"] := 1;
    сл[0]["второй"] := 2;
    сл[0]["третий"] := 3;
    сл[0]["четвёртый"] := 4;
    добавить(сл[0], "пятый", 5);
    если длина(арг) = 0 то начало
        длина(арг, 3);
        арг[0] := "адын";
        арг[1] := "дыва";
        арг[2] := "тыри";
    окончание;
    добавить(арг, "читыре");
    добавить(арг[длина(арг)-1], "-щесть");
    для каждого к из сл[0] от "пя" цикл 
        вывести(к, ": ", сл[0][к], " ");
    вывести(НС);
    для каждого к из сл[0] от "пя" обратный цикл 
        вывести(к, ": ", сл[0][к], " ");
    вывести(НС);
    для каждого ц из арг от 1 цикл 
        вывести(арг[ц], " ");
    вывести(НС);
    для каждого ц из арг обратный цикл 
        вывести(арг[ц], " ");
    вывести(НС);
    к := "012АБВ";
    для каждого ц из к цикл 
        вывести(ц, " ");
    вывести(НС);
    для каждого ц из к обратный цикл 
        вывести(ц, " ");
    вывести(НС);
окончание.
