программа Литерал;

    функция вывестиТочку(тчк: Точка): целое;
    начало
        вывести(формат("{г: %ц, в: %ц}"++НС, тчк.г, тчк.в));
    окончание;

константа
    т: Точка = {г = 0, в = 0};

переменные
    к: целое = 5;
    сбт: Событие = {что = сбтКлНаж, код = 27, инфо = сскШифт | сскКтрл};
    тчк: Точка;
    фиг: структура
        вид: строка;
        цвет: целое;
        положение: Точка;
        размер: Размер;
    окончание;
    мт: МассивТочек;
    сл: словарь Точек ключ строка;
    с: строка;
    втр: строка = "вторая";
начало
    тчк := {г = к+10, в = 30};
    вывестиТочку(тчк);
    вывестиТочку({г = к+5, в = 25});
    вывести(сбт.что, НС, сбт.код, НС, сбт.инфо, НС, сбт.г, НС, сбт.в, НС, НС);
    фиг := {
        вид = "эллипс",
        цвет = $FFFFFF,
        положение = {г = 10, в = 20},
        размер = {г = 25, в = 50}};    
    вывести(фиг.вид, НС, фиг.цвет, НС);
    вывестиТочку(фиг.положение);
    вывестиТочку(фиг.размер);
    вывести(НС);
    мт := [{г = к+5, в = 20}, тчк, {г = к+15, в = 40}, фиг.размер];
    для к от 0 до длина(мт)-1 цикл вывестиТочку(мт[к]);
    вывести(НС);
    к := 5;
    сл := {
        втр: {г = 0, в = 0},
        "пер"++"вая": {г = к+5, в = 20},
        втр: тчк,
        "третья": {г = к+15, в = 40},
        "четвертая": фиг.размер};
    для каждой с из сл цикл начало
        вывести(с ++ ": ");
        вывестиТочку(сл[с]);
    конец;
    ничего;
окончание.
