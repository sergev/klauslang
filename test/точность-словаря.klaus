программа ТочностьСловаря;
константа
    тчн = 0.01;
переменная
    к: дробное;
    сл: словарь строк ключ дробное точность тчн;
начало
    сл[0] := "Нуль";
    сл[0.001] := "Снова нуль";
    сл[0.02] := "Две сотых";
    для каждого к из сл цикл начало 
        вывести(формат("%6о: %с", к, сл[к]), нс);
    окончание;
    очистить(сл);
    вывести(длина(сл));
окончание.
