программа Вставка;
переменные
    м: массив целых;
    сл: словарь строк ключ строка;
    с: строка;
начало
    // массив
    вставить(м, 0, 10);
    вставить(м, 0, 20);
    вставить(м, 0, 30);
    вставить(м, 1, 1);
    вставить(м, 4, 2);
    удалить(м, 1, 2);
    // словарь
    вставить(сл, "один", "ОДИН");
    удалить(сл, "один");
    // строка
    с := "one";
    вставить(с, 0, "twenty ");
    вставить(с, 10, " million");
    вставить(с, 10, " point five");
    удалить(с, 10, 6);
    с := "один";
    вставить(с, 0, "двадцать ");
    вставить(с, 25, " миллион");
    вставить(с, 25, " с половиной");
    удалить(с, 25, 3);
    // будет ошибка
    // вставить(с, 1, "что-то");
    // удалить(с, 25, 4);
    ничего;
окончание.
