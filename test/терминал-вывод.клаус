программа ТерминалВывод;
переменная
    И: целое;
начало
    размерЭкрана(100, 30);
    очиститьЭкран();
    скрытьКурсор();
    для И от 0 до 23 цикл
        вывести("OOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOO", НС);
    курсор(5, 2);
    цветФона(8);
    очиститьСтроку(0);
    курсорВерт(4);
    очиститьСтроку(-1);
    курсорВерт(6);
    очиститьСтроку(1);
    курсор(0, 8);
    очиститьСтроку(0);
    цветФона(7);
    цветШрифта(0);
    вывести("тест");
    стильШрифта(1+2);
    вывести(" тест");
    стильШрифта(4);
    вывести(" тест");
    стильШрифта(1);
    вывести(" тест");
    стильШрифта(8);
    вывести(" тест");
    цветФона(цвет256($c0, $c0, $c1));
    стильШрифта(0);
    вывести(" тест");
    курсор(0, 25);
    сброситьАтрибуты();
    вывести("Нажмите ENTER...");
    ввести();
    показатьКурсор();
    очиститьЭкран();
окончание.
