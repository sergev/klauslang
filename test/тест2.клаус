программа Пример(арг: массив строк);

процедура тест(вых п: целое);
начало
    п := 800;
окончание;

константа
  К2 = (-10+3*2-1+2)*2^2+20 > 0 || 3 < 15^(1/2);
  К1 = 123;
  К3 = да;
  К4 = ""№0д"абвг"№0a; 
  K6 = `2019-03-2 16:00:1.123`;
  К7 = `2019-03-2`;
  К8 = `14:30`;
  К9 = `12`;
  К10 = !К2;

переменная
  В: дробное = 1;
начало
  К4 := "фыа";
  В += 2;
  вернуть К1;
окончание.
