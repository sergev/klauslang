программа ЕщеТест;

тип
    тСтр = структура
      с1: строка;
      с2: строка;
    окончание;

    функция заполнить: тСтр;
    переменная
        стр: тСтр;
    начало
        стр.с1 := "заполнено";
        стр.с2 := "не заполнено";
        вернуть стр;
    окончание;

    функция убавить(п: строка): строка;
    начало
        п := п ++ " убавить";
        вернуть п;
    окончание;

    функция умножить(п1, п2: целое; вв оп1: строка; вых оп2: строка): целое;
    переменная
        х: целое = 1;
        у: дробное;
    
        процедура прибавить(вв п1: строка);
        начало
            п1 := п1 ++ " прибавить";
            х := 10;
            У := 123.45;
        окончание;
        
    начало
        оп1 := оп1 ++ " умножить";
        прибавить(оп1);
        оп2 := строка(У);
        оп2 := "но " ++ убавить("не " ++ оп2);
        вернуть п1*п2;
    окончание;

переменная
    п: целое;
    рез: строка;
    стр: тСтр;
начало
    п := 2;
    стр.с1 := "сложить";
    стр.с2 := "вычесть";
    стр := заполнить();
    п := умножить(п, 2, стр.с1, стр.с2) + 2;
    рез := строка(п);
    вернуть 0;
окончание.
