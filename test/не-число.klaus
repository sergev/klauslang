программа ЧислоНеЧисло;
переменная
    д1: дробное = 0/0;
    д2: дробное = 0;
    беск: дробное;
начало
    вывести(формат("Меня зовут %1:s %0:s. Мне %2:i лет.", "Иван", "Иванов", 15));
    вывести(нечисло(минДробное), НС);
    вывести(дроб(123.45), НС);
    вывести(д1, " и ", д2);
    //если округл(д1) = д2 то вывести(" равны") иначе вывести(" не равны");
    вывести(НС);
    беск := 1/0;
    вывести(беск, НС);
    вывести(конечно(беск), НС);
    вывести(-беск, НС);
    вывести(момент("Nan"), НС);
    вывести(момент(беск), НС);
    вывести("---", НС);
    вывести(Pi, НС);
    д1 := Pi/3;
    вывести(cos(д1), НС);
    вывести(sin(д1), НС);
    вывести(tg(д1), НС);
    вывести(arccos(cos(д1)), НС);
    вывести(arcsin(sin(д1)), НС);
    вывести(arctg(tg(д1)), НС);
    вывести(ln(д1), НС);
    вывести(exp(ln(д1)), НС);
    вывести(exp(1), НС);
окончание.
