программа ПредварительноеОпределение;

тип Т1 = словарь целых ключ строка;
тип Т2 = словарь целых ключ строка;

переменная
    м: словарь целых ключ строка;
    глубина: целое = 0;

    процедура вторая(а/б: Т1); дальше;
    
    процедура первая();
    начало
        если глубина > 9 то вернуться;
        глубина += 1;
        вывести("первая", НС);
        вторая(м);
    окончание;
    
    процедура вторая(а/б: Т1);
    начало
        если глубина > 9 то вернуться;
        глубина += 1;
        вывести("вторая", НС);
        первая();
    окончание;

начало
    первая();
окончание.
