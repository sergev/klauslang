программа ТестДля;
тип
    Контакт/Контактов = структура
        фамилия: строка;
        имя: строка;
        отчество: строка;
        телефон: строка;
        почта: строка;
    окончание;
переменная
    контакты: массив Контактов;
начало
    длина(контакты, 2);
    для контакты[0] начало
        фамилия := "Иванов";
        имя := "Иван";
        отчество := "Иванович";
        телефон := "1234567";
        почта := "ivan@dev.null";
    конец;
    для контакты[1] начало
        фамилия := "Петров";
        имя := "Пётр";
        отчество := "Петрович";
        телефон := "7654321";
        почта := "petr@dev.null";
    конец;
    ничего;
окончание.
