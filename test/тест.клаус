программа Пример;

тип
    МС = массив строк;

процедура вывестиМассив(м: МС);
переменные
    к: целое;
начало
    если длина(м) = 0 то вывести("Пустой массив", НС)
    иначе для каждого к из м цикл вывести(м[к], НС);
окончание;

переменная
    м: МС;
    о: объект = пусто;
начало
    вернуть -3;
    вывестиМассив(пусто);
    вывестиМассив(["а", "б"]);
    м := ["Иванов", "Петров", "Сидоров"];
    вывестиМассив(м);
    м := пусто;
    вывестиМассив(м);
окончание.
