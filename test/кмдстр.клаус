программа Пустая(арг: массив строк);
константа
    Пи = 3.14159265;
переменная
    п1: строка = "какая-то строка"№0д№0а"А тут ещё какая-то ""вторая"" строка"№07;
    п2: целое = 10;
    п3: дробное = 123.4;
    п4: момент = `1 12:30`;
    п5: логическое = нет;
    м: массив строк;
    сл: словарь массивов строк ключ строка;
    стр: структура
      п1: строка;
      п2: дробное;
      п3: массив строк;
    окончание;
начало
    м := арг;
    п1 := 'a';
    п2 += 12343;
    арг[0] := "Hello, world!";
    м[0] := "Ещё какой-то текст...";
    сл["арг"] := арг;
    сл["м"] := м;
    м := сл["арг"];
    м := сл["брг"];
    стр.п1 := "Some text";
    стр.п2 := Пи;
    стр.п3 := арг;
    стр.п3[0] := "More text";
    ничего; 
окончание.
